{
  "name": "Technitium DNS",
  "version": "1.0.2",
  "slug": "technitium_dns",
  "description": "Technitium DNS Server als Home Assistant Add-on",
  "url": "https://github.com/PiperNigrum/ha_addons",
  "arch": ["armv7", "aarch64", "amd64"],
  "startup": "services",
  "boot": "auto",
  "map": [
    "addon_config:rw",
    "share:rw"
  ],
  "environment": {
    "APPDATAPATH": "/data"
  },
  "ports": {
    "5380/tcp": 5380,
    "53/tcp": 53,
    "53/udp": 53
  },
  "options": {},
  "schema": {}
}